<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,force:lightningQuickAction" access="global" >
	<aura:handler event="c:Show360AssistLeads" action="{!c.hideSearch}"/> 
    <aura:handler name="init" value="{!this}" action="{!c.hideSearch}"/>
    <div aura:id="searchDiv" class="slds-hide">
    	<c:Search360AssistLeads />
    </div>    
    <div aura:id="searchButton">
    	<lightning:buttonIcon iconName="utility:search" onclick="{!c.showSearch}" alternativeText="Search"/>
    </div>    
    <c:Tams360AssistLeadCmp />	
</aura:component>